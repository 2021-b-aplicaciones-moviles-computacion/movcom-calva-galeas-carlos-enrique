<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".RegistrarsePor">


    <Button
        android:id="@+id/aicCrearCuenta2"
        android:layout_width="243dp"
        android:layout_height="65dp"
        android:layout_marginBottom="272dp"
        android:backgroundTint="@color/green_ezfood"
        android:text="Crear Cuenta"
        android:textColor="@color/white"
        app:cornerRadius="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="320dp"
        android:layout_height="202dp"
        android:layout_marginTop="60dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_logo_ezfood" />

    <Button
        android:id="@+id/googleBoton"
        android:layout_width="244dp"
        android:layout_height="69dp"
        android:layout_marginTop="68dp"
        android:backgroundTint="@color/orange_ezfood"
        android:text="Google"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="@+id/aicCrearCuenta2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/aicCrearCuenta2"
        app:layout_constraintTop_toBottomOf="@+id/aicCrearCuenta2" />


</androidx.constraintlayout.widget.ConstraintLayout>